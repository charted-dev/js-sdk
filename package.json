{
  "name": "@ncharts/root",
  "description": "🐻‍❄️📦 Root project for @ncharts/sdk",
  "private": true,
  "version": "0.0.0-dev.0",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "generate:types": "yarn fmt && yarn lint && tsx --tsconfig ./scripts/tsconfig.json scripts/generate-types.ts",
    "build": "yarn fmt && yarn lint && rimraf dist && tsx --tsconfig ./scripts/tsconfig.json scripts/build.ts",
    "lint": "tsx --tsconfig ./scripts/tsconfig.json scripts/eslint.ts",
    "fmt": "prettier --config ./.prettierrc.json --write **/*.{md,ts,js,tsx,mdx,yaml,yml,json}"
  },
  "devDependencies": {
    "@actions/core": "1.10.0",
    "@augu/eslint-config": "3.0.0",
    "@augu/tsconfig": "1.1.1",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "@types/signale": "1.4.4",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "log-symbols": "5.1.0",
    "prettier": "2.8.7",
    "signale": "1.4.0",
    "testcontainers": "9.1.1",
    "tsx": "3.12.2",
    "typescript": "4.9.4"
  }
}
